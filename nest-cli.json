{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      "**/*.html,",
      "**/template.*",
      "**/*.png",
      "**/*.css",
      "**/*.config.json",
      "**/*.xlsx",
      "**/*.js",
      "**/*.hbs"
    ]
  }
}
